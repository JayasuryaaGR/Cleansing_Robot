<?xml version='1.0'?>
<robot name="artpark_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:include filename="$(find artpark_description)/urdf/artpark_robot.gazebo" />
  <xacro:include filename="$(find artpark_description)/urdf/materials.xacro" />

  <gazebo>
    <static>false</static>
  </gazebo>

  <link name="base_link"/>

  <link name="chasis">
    <collision name='chasis_collision'>
      <geometry>
        <box size=".6 .6 .1"/>
      </geometry>
    </collision>
    <visual name='chasis_visual'>
      <geometry>
        <box size=".6 .6 .1"/>
      </geometry>
      <material name="red" />
    </visual>
    <inertial name="chasis_link_inertial">
      <mass value="2"/>
      <inertia ixx="0.06167" ixy="0.0" ixz="0.0" iyy="0.06167" iyz="0.0" izz="0.12"/>
    </inertial>
  </link>

  <joint name="chasis_joint" type="fixed">
    <parent link="base_link"/>
    <child link="chasis"/>
    <origin xyz="0 0 0.15" rpy="0 0 0"/>
  </joint>
  
  <link name="body">
    <collision name='body_collision'>
      <geometry>
        <box size=".6 .6 .8"/>
      </geometry>
    </collision>
    <visual name='body_visual'>
      <geometry>
        <box size=".6 .6 .8"/>
      </geometry>
      <material name="red" />
    </visual>
    <inertial name="base_link_inertial">
      <mass value="2"/>
      <inertia ixx="0.16667" ixy="0.0" ixz="0.0" iyy="0.16667" iyz="0.0" izz="0.12"/>
    </inertial>
  </link>

  <joint name="body_joint" type="continuous">
    <parent link="chasis"/>
    <child link="body"/>
    <origin xyz="0 0 .45" rpy="0 0 0"/>
    <axis xyz="0.0 0.0 1.0"/>
    <dynamics damping="1"/>
  </joint>

  <transmission name="body_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="body_joint">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="body_motor">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

