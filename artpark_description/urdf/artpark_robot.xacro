    </inertial>
  </link>

  <joint name="camera_joint" type="fixed">
    <parent link="body"/>
    <child link="camera"/>
    <origin xyz=".3 0 .015" rpy="0.0 0.0 0.0"/>   <!--rpy = "0 .4 0"-->
